    "mockjs": "^1.1.0",
    "postcss": "^8.2.8",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "typescript": "^4.6.4",
    "unplugin-icons": "^0.14.7",
    "vite": "^2.9.9",
    "vite-aliases": "^0.5.1",
