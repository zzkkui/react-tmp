  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "ahooks": "^3.5.2",
    "antd": "^4.14.1",
    "axios": "^0.21.1",
    "lodash": "^4.17.21",
    "lottie-web": "^5.9.6",
    "normalize.css": "^8.0.1",
    "qs": "^6.9.6",
    "react": "^17.0.1",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.1",
    "react-query": "^3.39.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "recoil": "^0.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run lint:script",
      "prettier .prettierrc.js --write"
    ],
    "*.{less}": [
      "npm run lint:style"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
